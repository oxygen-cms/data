{
    "name": "oxygen/data",
    "description": "Oxygen's Data Layer",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.2",
        "jorrit77/laravel-doctrine": "dev-master",
        "seld/jsonlint": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Data\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen Data",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Validation",
                "class": "Oxygen\\Data\\Validation\\Laravel\\ValidationServiceProvider",
                "description": "Custom Validation Framework"
            }
        ]
    }
}
