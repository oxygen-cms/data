{
    "name": "oxygen/data",
    "description": "Oxygen's Data Layer",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "^10.0",
        "laravel-doctrine/orm": "^2.0",
        "shapecode/doctrine-dbal-datetimeutc": "^3.0",
        "webmozart/assert": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Data\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oxygen\\Data\\DataServiceProvider"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "phpstan/extension-installer": "^1.2"
    },
    "scripts": {
        "test": "vendor/bin/phpspec run && vendor/bin/phpstan analyze src --level 5"
    }
}
